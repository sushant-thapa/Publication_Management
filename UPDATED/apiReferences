API REFERENCES FOR "PROJECT ON PUBLICATION LIST MANAGEMENT"

1.
	endpoint: localhost:3001/addNewPublication
	
	//required json file can have any number of fields
	required variables: json:		//due to the use of nonrelational database, json file may have different attributes
	{
		user_email: string,
	    title: string,
	    date: string,
		isbn: integer,
		impact_factor: integer,
		type_of_article: string,
		volume: string,
		name_of_journal: string,
		page_number: string,
		location_of_conference: string,
		article_link: string
		academic_level: string,
		doi: string,
		authors: [
			{
				first_name: string,
				last_name: string,
				email: string
			},
			{ 
				first_name: string, 
				last_name: string,
				email: string 
			}
		    ]	
	}
	
	returns: none

2.
	endpoint: localhost:3001/deletePublication
	
	required variables:
		title : string
	
	returns: none

3.
	endpoint: localhost:3001/list
	
	required variables:
		type_of_article: string

	returns:array of json	//attributes of json in returned array depends on the attributes used while adding new publications
		
				
		[
			{
				user_email: string,
	    		title: string,
	    		date: string,
				isbn: integer,
				impact_factor: integer,
				type_of_article: string,
				volume: string,
				name_of_journal: string,
				page_number: string,
				location_of_conference: string,
				article_link: string
				academic_level: string,
				doi: string,
				authors: [
					{
						first_name: string,
						last_name: string,
						email: string
					},
					{ 
						first_name: string, 
						last_name: string,
						email: string 
					}
					]	
			},
	 		{
				user_email: string,
	    		title: string,
	    		date: string,
				isbn: integer,
				impact_factor: integer,
				type_of_article: string,
				volume: string,
				name_of_journal: string,
				page_number: string,
				location_of_conference: string,
				article_link: string
				academic_level: string,
				doi: string,
				authors: [
					{
						first_name: string,
						last_name: string,
						email: string
					},
					{ 
						first_name: string, 
						last_name: string,
						email: string 
					}
					]	
			}
		]

4.
	endpoint:localhost: 3001/showOneArticle

	required variables:
		title : string

	returns: json:		//attributes of json depend on the attributes used while adding new publications
	{
		user_email: string,
	    title: string,
	    date: string,
		isbn: integer,
		impact_factor: integer,
		type_of_article: string,
		volume: string,
		name_of_journal: string,
		page_number: string,
		location_of_conference: string,
		article_link: string
		academic_level: string,
		doi: string,
		authors: [
			{
				first_name: string,
				last_name: string,
				email: string
			},
			{ 
				first_name: string, 
				last_name: string,
				email: string 
			}
		    ]	
	}




